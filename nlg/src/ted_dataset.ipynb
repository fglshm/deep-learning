    "import pickle\n",
    "\n",
    "def load_data(speech_type):\n",
    "    pkl_filename = 'ted.{}.vocab.pkl'.format(speech_type)\n",
    "    with open(pkl_filename, 'rb') as f:\n",
    "        data = pickle.load(f)\n",
    "    \n",
    "    all_words, word_to_id, id_to_word = data\n",
    "    return all_words, word_to_id, id_to_word"
